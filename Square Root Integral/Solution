import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here

        // taking input
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();

		// finding square root
		int sq = (int)( Math.sqrt(N) );

		// printing square root 
		System.out.println(sq);
	}
}
