public class Solution {

    public static int arrayRotateCheck(int[] arr){
    	//Your code goes here
        if(arr.length==0){
            return 0;
        }
        int m = arr[0];
        int minin = 0;
        for(int i=0; i<arr.length; i++){
           if(arr[i]<m){
               m=arr[i];
               minin = i;
           }
        }

      return minin;

    }

}
