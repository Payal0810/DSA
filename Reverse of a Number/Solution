import java.util.Scanner;

import java.util.*;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();

		int temp = N;
		int sum = 0;

		while(N>0){
			int rem = N%10;
            sum = sum*10+rem;
			N=N/10;
		}
		System.out.println(sum);
	}
}
