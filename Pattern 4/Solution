import java.util.*;
public class Solution {
	public static void main(String[] args) {
		//Your code goes here

		// taking input 
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();

		// loop for rows 
		for(int i=1; i<=N; i++){
            
			// starting character 
			char p = (char)('A'+N-i);

			// loop for columns 
			for(int j=1; j<=i; j++){	
				System.out.print(p);
				p++;
			}

			// next line 
			System.out.println();
		}
	}
}
