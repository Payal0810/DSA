import java .util.Scanner;
public class Solution {
	public static void main(String args[]) {
        

        // write your code logic !!

        // taking input 
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();

		// loop for rows 
		for(int i=1; i<=N; i++){

			for(int j=i; j<=N; j++){
				System.out.print(j+" ");
			}

			for(int j=1; j<i; j++){
				System.out.print(j+" ");
			}

			System.out.println();
		}
        
	}
}
