
public class Solution {

	public static void rowWiseSum(int[][] mat) {
		//Your code goes here
		int rows = mat.length;
		if(rows==0){
			return;
		}
		int cols = mat[0].length;
		for(int i=0; i<rows; i++){
			long sum =0;
			for(int j=0; j<cols; j++){
				sum=sum+mat[i][j];
			}
			System.out.print(sum+" ");
		}
		System.out.println();
	}

}
